{
  "name": "@estrella/ng-tailwindcss",
  "version": "1.0.0",
  "description": "Builds an Angular project with Tailwindcss. It sets up Angular routing, basic scafolding and blank page.",
  "scripts": {
    "new": "schematics .:ng-tailwindcss --debug=false",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "pack": "npm pack",
    "publish": "npm publish --access=public"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.0.1",
    "@angular-devkit/schematics": "^12.0.1",
    "@schematics/angular": "^12.0.1",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0"
  }
}
